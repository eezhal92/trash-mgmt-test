<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Home</title>
</head>
<body>
  <h1>Log</h1>
  <div id="logs-container">
    <% logs.forEach(function (log) { %>
    <div class="log-item">
      <p>Temp: <%= log.temp %></p>
      <p>Hum: <%= log.hum %></p>
      <hr>
    </div>
    <% }) %>
  </div>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.0.4/socket.io.js"></script>
  <script src="/js/app.js"></script>
</body>
</html>